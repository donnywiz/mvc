{% extends 'base.html.twig' %}

{% block title %}Card Game{% endblock%}


{% block content %}
<div class="main-content">

<h1> {{ title }} </h1>

{% for object in data.showDeck %}
    <p>
        #{{ loop.index }}
        {{ object.suit }}
        {{ object.rank }}
        ({{ object.value }})
    </p>
{% endfor %}

    </div>
</div>

</div>
{% endblock %}
